{
  "name": "tvkit",
  "version": "0.14.0",
  "description": "Proxy to run a modern dev server in old browsers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfanger/tvkit.git"
  },
  "type": "module",
  "bin": {
    "tvkit": "bin/tvkit.js"
  },
  "scripts": {
    "dev": "pm2-dev \"bin/tvkit.js serve --port 3011 --browser ie11\"",
    "dev:latest": "pm2-dev \"bin/tvkit.js serve --browser='last 1 chrome versions'\"",
    "lint": "concurrently -c \"#c596c7\",\"#3074c0\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn src",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "eslint --ext=js,ts,svelte --fix src && prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.(c)?js": [
      "eslint --max-warnings 0 --no-ignore"
    ]
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.4",
    "@types/caniuse-lite": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-triple": "^1.1.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "acorn": "^8.11.2",
    "acorn-walk": "^8.3.0",
    "browserslist": "^4.22.1",
    "caniuse-lite": "^1.0.30001563",
    "core-js": "^3.33.2",
    "core-js-compat": "^3.33.2",
    "cross-browser-polyfill": "^1.0.5",
    "custom-event-polyfill": "^1.0.7",
    "express": "^4.18.2",
    "fast-text-encoding": "^1.0.6",
    "http-proxy-middleware": "^2.0.6",
    "intersection-observer": "^0.12.2",
    "magic-string": "^0.30.5",
    "node-html-parser": "^6.1.11",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^9.3.0",
    "proxy-polyfill": "^0.3.2",
    "regenerator-runtime": "^0.14.0",
    "rollup": "^4.5.0",
    "systemjs": "^6.14.2",
    "terser": "^5.24.0",
    "unorm": "^1.6.0",
    "whatwg-fetch": "^3.6.19",
    "yargonaut": "^1.1.4",
    "yargs": "^17.7.2"
  }
}
